{
  "name": "api-functions-v4-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "start:dev": "concurrently 'npm run start:azurite' 'npm run start'",
    "test": "echo \"No tests yet...\"",
    "start:azurite": "azurite -s -l './azurite' -d './azurite/debug.log'"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0-alpha.7",
    "azurite": "^3.24.0",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "rimraf": "^5.0.0",
    "typescript": "^4.0.0"
  },
  "main": "dist/src/functions/*.js"
}
